{
    "name": "multiinfo/notification-bundle",
    "description": "MIT Notification Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "MIT",
            "email": "med.ahamada@mit-agency.com"
        }
